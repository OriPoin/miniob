{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "gdb-observer-cli",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-P", "cli"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/bin",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "gdb-observer-mysql",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-P", "mysql"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/bin",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "gdb-observer-mvcc",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-t", "mvcc"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/bin",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "lldb-observer-cli",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-P", "cli"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "lldb-observer-mysql",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-P", "mysql"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "lldb-observer-mvcc",
            "program": "${workspaceFolder}/${defaultBuildTask}/bin/observer",
            "args": ["-f", "${workspaceFolder}/etc/observer.ini", "-t", "mvcc"],
            "cwd": "${workspaceFolder}/${defaultBuildTask}/"
        }
    ]
}